exceptions = []
expire = 7200
history-size = 10
rtr-listen = [{% for listener in routinator_listener_rtr %} "{{ listener }}", {% endfor %}]
http-listen = [{% for listener in routinator_listener_http %} "{{ listener }}", {% endfor %}]
log = "default"
log-level = "{{ routinator_log_level }}"
refresh = 3600
repository-dir = "{{ routinator_path }}/repository"
retry = 600
rsync-command = "rsync"
rsync-timeout = 600
strict = false
syslog-facility = "daemon"
tal-dir = "{{ routinator_path }}/tals"
validation-threads = {{ routinator_validation_threads }}
